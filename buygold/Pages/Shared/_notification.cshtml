@if (TempData["success"] != null)
{
	<link rel="stylesheet" href="~/lib/iziToast/dist/css/iziToast.css" />
	<script src="~/lib/jquery/dist/jquery.min.js" type="text/javascript"></script>
	<script src="~/lib/iziToast/dist/js/iziToast.js" type="text/javascript"></script>
	<script style="font-family:'Roboto','Vazirmatn', sans-serif;" type="text/javascript">
		iziToast.success({ title: '@TempData["success"]' });
	</script>
}
